
/* >= nefunguje spravne
** 
** chyba nastava aj v pripade, ak 
** porovnavas cislo "a" operatorom != za ktorym
** nasleduje double v tvare xx.xx (segmentation fault)
*/
class Main {
	static void run() {
		double a = 1.5;

		if (a >= 2) {										
			ifj16.print ("1" + "\n");
		} 

		else {
			ifj16.print ("2" + "\n");
		} 
	}
}
