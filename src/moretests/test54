
/* v podmienke if ak porovnavas cislo "a" akymkolvek 
** relacnym operatorom s cislom bez desatinej ciarky 
** je to ok, rovnako je to ok aj ked porovnavas
** cislo "a" s cislom s desatinnou ciarkou
** chyba nastava v jedinom pripade a to vtedy, ak 
** porovnavas cislo "a" operatorom != za ktorym
** nasleduje double v tvare xx.xx
*/
class Main {
	static void run() {
		double a = 1.5;

		if (a != 1.5) {					//segmentation fault					
			ifj16.print ("1" + "\n");
		} 

		else {
			ifj16.print ("2" + "\n");
		} 
	}
}