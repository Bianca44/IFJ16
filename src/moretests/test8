class Main {
	static void run (){
		boolean a = true;
		boolean b = false;

		if (a && b) {
			ifj16.print("a && b");
		}

		else if (a || b) {
			ifj16.print("a || b");
		}

		else {
			ifj16.print("both are false");
		}

		if (a && a) {
			ifj16.print("a is true");
		}

		else if (b && b) {
			ifj16.print("b is true");
		}

		else {
			ifj16.print("both are false");
		}
	}
}