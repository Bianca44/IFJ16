class Main
{
    static void run()
    {
        ifj16.print("Zadaje slovo: ");
        String h = ifj16.readString();
        ifj16.print("Zadaje dalsie slovo: ");
        String g = ifj16.readString();

        int vysledkok = ifj16.find(h,g);
        ifj16.print("Vysledok ? " + vysledkok);


    }
}
