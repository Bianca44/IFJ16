
class Main
{
    static void run()
    {
    	ifj16.print("Zadajte int: ");
        int a = ifj16.readInt();
        ifj16.print("Zadajte double: ");
        double b = ifj16.readDouble();

        double vysledok = a!b;			//unexpected expression? skor lexikalna chyba 
        ifj16.print("vysledok = "+vysledok); 
    }
}
